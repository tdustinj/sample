<OrderRequest>
  <Version>string</Version>
  <TransactionHeader>
    <SenderID>string</SenderID>
    <ReceiverID>string</ReceiverID>
    <CountryCode>string</CountryCode>
    <LoginID>string</LoginID>
    <Password>string</Password>
    <!--Optional:-->
    <TransactionID>string</TransactionID>
    <!--Optional:-->
    <SSBranch>string</SSBranch>
    <!--Optional:-->
    <SSAccount>string</SSAccount>
    <!--Optional:-->
    <SSSuffix>string</SSSuffix>
  </TransactionHeader>
  <OrderHeaderInformation>
    <!--Optional:-->
    <BillToSuffix>string</BillToSuffix>
    <AddressingInformation>
      <CustomerPO>string</CustomerPO>
      <ShipToAttention>string</ShipToAttention>
      <!--Optional:-->
      <OrderContactName>string</OrderContactName>
      <!--Optional:-->
      <EndUserPO>string</EndUserPO>
      <!--Optional:-->
      <CustomerSalesOrder>string</CustomerSalesOrder>
      <!--Optional:-->
      <SpecialBidNumber>string</SpecialBidNumber>
      <!--Optional:-->
      <ShipTo>
        <!--You have a CHOICE of the next 2 items at this level-->
        <Address>
          <ShipToAddress1>string</ShipToAddress1>
          <!--Optional:-->
          <ShipToAddress2>string</ShipToAddress2>
          <!--Optional:-->
          <ShipToAddress3>string</ShipToAddress3>
          <ShipToCity>string</ShipToCity>
          <!--Optional:-->
          <ShipToProvince>string</ShipToProvince>
          <ShipToPostalCode>string</ShipToPostalCode>
          <!--Optional:-->
          <ShipToCountryCode>string</ShipToCountryCode>
          <!--Optional:-->
          <ShipToEmailAddress>string</ShipToEmailAddress>
        </Address>
        <Suffix>
          <ShipToSuffix>string</ShipToSuffix>
        </Suffix>
      </ShipTo>
    </AddressingInformation>
    <ProcessingOptions>
      <!--Optional:-->
      <CarrierCode>string</CarrierCode>
      <!--Optional:-->
      <AutoRelease>string</AutoRelease>
      <!--Optional:-->
      <ThirdPartyFreightAccount>string</ThirdPartyFreightAccount>
      <!--Optional:-->
      <KillOrderAfterLineError>string</KillOrderAfterLineError>
      <ShipmentOptions>
        <BackOrderFlag>string</BackOrderFlag>
        <SplitShipmentFlag>string</SplitShipmentFlag>
        <!--Optional:-->
        <SplitLine>string</SplitLine>
        <ShipFromBranches>string</ShipFromBranches>
        <!--Optional:-->
        <DeliveryDate>string</DeliveryDate>
      </ShipmentOptions>
      <!--Optional:-->
      <CreditCardProcessing>
        <paymentCode>st</paymentCode>
        <!--Optional:-->
        <cardType>s</cardType>
        <creditCardNumber>string</creditCardNumber>
        <expiryDate>2008-09-28</expiryDate>
        <cvvNumber>stri</cvvNumber>
        <cardHolderLastName>string</cardHolderLastName>
        <cardHolderNameInitial>s</cardHolderNameInitial>
        <cardHolderAddress>
          <!--Optional:-->
          <address>string</address>
          <!--Optional:-->
          <city>string</city>
          <!--Optional:-->
          <state>string</state>
          <zipcode>string</zipcode>
        </cardHolderAddress>
        <!--Optional:-->
        <authCoCode>str</authCoCode>
        <!--Optional:-->
        <authCode>string</authCode>
        <!--Optional:-->
        <authAmount>1000.00</authAmount>
      </CreditCardProcessing>
    </ProcessingOptions>
    <!--Optional:-->
    <DynamicMessage>
      <MessageLines>string</MessageLines>
    </DynamicMessage>
    <!--Optional:-->
    <Government>
      <!--Optional:-->
      <GovProgramType>string</GovProgramType>
      <GovEndUserType>string</GovEndUserType>
      <GovSolicitationNBR>string</GovSolicitationNBR>
      <GovEndUserData>string</GovEndUserData>
      <GovEndUserZip>string</GovEndUserZip>
      <!--Optional:-->
      <GovPublicPrivateSW>string</GovPublicPrivateSW>
    </Government>
    <!--Zero or more repetitions:-->
    <Attributes>
      <!--1 or more repetitions:-->
      <nameValue>
        <name>string</name>
        <value>string</value>
      </nameValue>
    </Attributes>
    <!--Optional:-->
    <CustomOrderPersist>
      <!--1 or more repetitions:-->
      <NameValue>
        <Name>string</Name>
        <Value>string</Value>
      </NameValue>
    </CustomOrderPersist>
  </OrderHeaderInformation>
  <OrderLineInformation>
    <!--You have a CHOICE of the next 2 items at this level-->
    <!--Optional:-->
    <ProductLine>
      <!--Optional:-->
      <SKU>string</SKU>
      <Quantity>string</Quantity>
      <!--Optional:-->
      <SpecialBid>
        <!--Optional:-->
        <SpecialBidNumber>string</SpecialBidNumber>
        <RequiredPrice>string</RequiredPrice>
      </SpecialBid>
      <!--Optional:-->
      <CustomerLineNumber>string</CustomerLineNumber>
      <!--Optional:-->
      <ReservedInventory>
        <ReserveCode>string</ReserveCode>
        <ReserveSequence>string</ReserveSequence>
      </ReservedInventory>
      <!--Optional:-->
      <CustomLinePersist>
        <!--1 or more repetitions:-->
        <NameValue>
          <Name>string</Name>
          <Value>string</Value>
        </NameValue>
      </CustomLinePersist>
      <!--Optional:-->
      <CustomerPartNumber>string</CustomerPartNumber>
      <!--Optional:-->
      <UPC>string</UPC>
      <!--Optional:-->
      <ManufacturerPartNumber>string</ManufacturerPartNumber>
      <!--Optional:-->
      <ShipFromBranchAtLine>string</ShipFromBranchAtLine>
      <!--Optional:-->
      <RequestedPrice>string</RequestedPrice>
      <!--Optional:-->
      <EndUserPrice>string</EndUserPrice>
      <!--Optional:-->
      <ResellerPriceOverride>string</ResellerPriceOverride>
    </ProductLine>
    <!--Optional:-->
    <CommentLine>
      <CommentText>string</CommentText>
    </CommentLine>
  </OrderLineInformation>
  <!--Optional:-->
  <ShowDetail>string</ShowDetail>
</OrderRequest>